<section>

  <div class="container t-center">

    <h1>Ironhack Cinema</h1>
    <p class="pb-l">click on a movie to check the showtimes!</p>

    <div class="wrapper">
      {{#each movie}}
        <div class="movie-card">
          <img src="{{image}}" alt="{{title}}">
          <h2>{{title}}</h2>
          <a href="/movies/{{id}}">See more</a>
        </div>
      {{/each}}
    </div>

  </div>

</section>
